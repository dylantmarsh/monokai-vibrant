{
  "name": "Monokai Vibrant",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#F6F6F6"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "string.comment"],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#FFD945"
      }
    },
    {
      "name": "Punctuation Template Expression",
      "scope": "punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end, punctuation.section.embedded",
      "settings": {
        "foreground": "#FF3F4F"
      }
    },
    {
      "name": "TemplateString",
      "scope": "meta.string.template",
      "settings": {
        "foreground": "#E5A422"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E373CE"
      }
    },
    {
      "name": "Embeded String Begin and End",
      "scope": ["string.embedded.begin", "string.embedded.end"],
      "settings": {
        "foreground": "#E373CE"
      }
    },
    {
      "name": "Embeded String",
      "scope": "string.embedded",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#E373CE"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#E373CE"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#FF3F4F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.readwrite", "variable.readwrite.other.block"],
      "settings": {
        "foreground": "#00a0e4"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#FF3F4F"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FF3F4F"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#FF3F4F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#19D1E5"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#00a0e4"
      }
    },
    {
      "name": "Variable Object",
      "scope": ["meta.block.switch.c", "variable.other.object"],
      "settings": {
        "foreground": "#00a0e4"
      }
    },
    {
      "name": "Other variable",
      "scope": ["variable.other.property", "variable.other.block"],
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#81F900"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#81F900"
      }
    },
    // Support function color changed to #19D1E5
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#19D1E5"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9700"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": ["function.support.builtin", "function.support.core"],
      "settings": {
        "foreground": "#81F900"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js",
        "entity.name.tag.class.jsx"
      ],
      "settings": {
        "foreground": "#FF3F4F"
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": ["entity.name.tag.class", "entity.name.tag.id"],
      "settings": {
        "foreground": "#19D1E5"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#81F900"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#19D1E5"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.variable"],
      "settings": {
        "foreground": "#19D1E5"
      }
    },
    {
      "name": "Json Property",
      "scope": "support.dictionary.json",
      "settings": {
        "foreground": "#19D1E5"
      }
    },
    {
      "name": "StyleSheet Property name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#6AEBF7"
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": ["unit.css", "unit.scss", "unit.less", "unit.sass"],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "StyleSheet Number",
      "scope": [
        "constant.numeric.css",
        "constant.numeric.scss",
        "constant.numeric.less",
        "constant.numeric.sass"
      ],
      "settings": {
        "foreground": "#E373CE"
      }
    },
    {
      "name": "StyleSheet Function",
      "scope": [
        "function.css",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON String",
      "scope": "structure.dictionary.property-name.json",
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#00a0e4"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#E542FF"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#56b6c2A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "",
        "foreground": "#696969"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#00a0e4"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Bold Punctuation",
      "scope": "punctuation.definition.bold.markdown",
      "settings": {
        "fontStyle": "",
        "foreground": "#919191"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF3F4F"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "fontStyle": "",
        "foreground": "#919191"
      }
    },
    {
      "name": "Markdown: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#6FD80D"
      }
    },
    {
      "name": "Markdown: Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#E542FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Raw",
      "scope": "markup.raw.inline.markdown, markup.raw.block.markdown",
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Markdown: List Punctuation",
      "scope": "punctuation.definition.list_item.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "JSDoc: Variable",
      "scope": "variable.other.jsdoc",
      "settings": {
        "foreground": "#9496a5"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "newOperator": "#FF3F4F",
    "stringLiteral": "#FFD945",
    "customLiteral": "#81F900",
    "numberLiteral": "#E373CE",
    "variable": "#bdbdbd",
    "variable.readonly": "#ffff",
    "variable.defaultLibrary": "#ffff"
  },
  "colors": {
    "activityBar.background": "#16171D",
    "activityBar.foreground": "#D7DAE0",
    "activityBarBadge.background": "#528bff",
    "activityBarBadge.foreground": "#F8FAFD",
    "button.background": "#528bff",
    "dropdown.background": "#1d1f23",
    "diffEditor.insertedTextBackground": "#00809B33",
    "dropdown.border": "#181A11",
    "editor.background": "#16171D",
    "editor.foreground": "#f8f8f0",
    "editorError.foreground": "#c24038",
    "editorRuler.foreground": "#484848",
    "editor.lineHighlightBackground": "#1F2328",
    "editor.selectionBackground": "#3E4451",
    "editorCursor.foreground": "#f8f8f0",
    "editor.findMatchBackground": "#42557B",
    "editor.findMatchHighlightBackground": "#314365",
    "editorGroup.border": "#3B3C40",
    "editorGroupHeader.tabsBackground": "#21252B",
    "editorIndentGuide.background": "#3B4048",
    "editorLineNumber.foreground": "#495162",
    "editorWhitespace.foreground": "#484a50",
    "editorHoverWidget.background": "#21252B",
    "editorHoverWidget.border": "#181A1F",
    "editorSuggestWidget.background": "#21252B",
    "editorSuggestWidget.border": "#181A1F",
    "editorSuggestWidget.selectedBackground": "#2c313a",
    "editorWidget.background": "#21252B",
    "input.background": "#1d1f23",
    "list.activeSelectionBackground": "#2c313a",
    "list.activeSelectionForeground": "#d7dae0",
    "list.focusBackground": "#383E4A",
    "list.hoverBackground": "#292d35",
    "list.highlightForeground": "#C5C5C5",
    "list.inactiveSelectionBackground": "#2c313a",
    "list.inactiveSelectionForeground": "#d7dae0",
    "scrollbarSlider.background": "#4E566680",
    "scrollbarSlider.activeBackground": "#747D9180",
    "scrollbarSlider.hoverBackground": "#5A637580",
    "sideBar.background": "#191B20",
    "sideBarSectionHeader.background": "#282c34",
    "statusBar.background": "#1b1c23",
    "statusBar.foreground": "#fff",
    "statusBarItem.hoverBackground": "#2c313a",
    "statusBar.noFolderBackground": "#21252B",
    "statusBar.debuggingBackground": "#21252B",
    "tab.activeBackground": "#2c313a",
    "tab.border": "#181A1F",
    "tab.activeBorderTop": "#007DD8",
    "tab.inactiveBackground": "#21252B",
    "titleBar.activeBackground": "#282c34",
    "titleBar.activeForeground": "#9da5b4",
    "titleBar.inactiveBackground": "#282C34",
    "titleBar.inactiveForeground": "#6B717D",
    "editorInlayHint.foreground": "#838594",
    "editorInlayHint.background": "#262831",
    "editorHoverWidget.highlightForeground": "#ffffff"
  }
}
